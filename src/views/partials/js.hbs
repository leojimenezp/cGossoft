<script>

$( ".calcular_total" ).keyup(function() {
  
    cantidad_costo_fijo = $("#cantidad_costo_fijo").val();
    precio_costo_fijo = $("#precio_costo_fijo").val();

    total = (cantidad_costo_fijo*precio_costo_fijo);
    
    $("#total_costo_fijo").val(total);
    
});

</script>

